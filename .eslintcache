onst formSubmit = async () => {\n        let formValues = {\n            email: email,\n            password: password,\n        }\n\n        await axios.post('http://localhost:8000/sign-in', formValues)\n        .then(response => {\n            if (response.data === \"Email or Password is incorrect\"){\n                setIncorrect(true)\n            } else if (response.data.auth){\n                let tokenDecoded = jwt_decode(response.data.token)\n                setIncorrect(false)\n                props.signinUserAction({tokenDecoded, token: response.data.token})\n                history.push('/')\n            }\n        })\n    }\n\n    const handleSubmit = (e) => {\n        e.preventDefault()\n    }\n\n    return(\n        <>\n        <Header />\n        <div className=\"signin-container\">\n            <h1>Connexion</h1>\n            <form  onSubmit={handleSubmit}>\n                <div className=\"signin-email\">\n                    <label>Votre email</label>\n                    <input type=\"email\" name=\"email\" id=\"email\" required onChange={e => setEmail(e.target.value)}/>\n                </div>\n                <div className=\"signin-pwd\">\n                    <label>Votre mot de passe</label>\n                    <input type=\"password\" name=\"password\" id=\"password\" required onChange={e => setPassword(e.target.value)}/>\n                </div>\n                <div className=\"signin-btn\">\n                    <button onClick={formSubmit}>connexion</button>\n                </div>\n            </form>\n        </div>\n        </>\n    )\n}\n\nconst mapDispatchToProps = { signinUserAction }\nconst mapStateToProps = (state) => ({\n    signinStore: state.signin,\n  });\n\nexport default connect(mapStateToProps,mapDispatchToProps)(Signin)","/Users/niddam/Desktop/react/ecommerce-projet-final-formation/src/storeRedux/action/signinUserAction.js",[],"/Users/niddam/Desktop/react/ecommerce-projet-final-formation/src/storeRedux/reducer/index.js",[],